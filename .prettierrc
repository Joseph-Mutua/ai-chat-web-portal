{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "importOrder": [
    "^react",
    "^react-native",
    "^@expo/(.*)$",
    "^(expo|date-fns)(.*)$",
    "^@/components/(.*)$",
    "^@/constants/(.*)$",
    "^@/hooks(.*)$",
    "^@/messages(.*)$",
    "^@/utils(.*)$",
    "^@/types(.*)$",
    "^@/enums(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
